{
  "name": "ts-graphql",
  "version": "0.1.1",
  "main": "lib/index.js",
  "author": "stephentuso",
  "files": [
    "lib/"
  ],
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "tslint --fix src/**/*.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.2",
    "@types/graphql": "^14.0.1",
    "@types/jest": "22",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.10",
    "@types/pify": "^3.0.2",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "jest": "22",
    "pify": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "reflect-metadata": "^0.1.12"
  },
  "peerDependencies": {
    "graphql": "^14.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/*.d.ts",
      "!**/__tests__/**",
      "!testUtils.ts"
    ]
  }
}
